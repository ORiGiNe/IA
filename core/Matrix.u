class Object.Matrix {
    // la matrix, une liste de liste
    var matrix;
    var x;
    var y;

    function init (x, y, v) {
        matrix = [];
        this.x = x;
        this.y = y;

        for (var i in x) {
            this.matrix << [];

            for (var j in y)
                this.matrix[i] << v;
       };
    };

   
    // sucre syntaxique
    function '[]' (i) {
        this.matrix[i];
    };

    function asString {
        this.matrix.asString;
    };
};
